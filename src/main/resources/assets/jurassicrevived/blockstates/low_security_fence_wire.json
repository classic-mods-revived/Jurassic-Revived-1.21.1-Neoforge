{
  "multipart": [
    { "apply": { "model": "jurassicrevived:block/low_security_fence_wire" } },

    { "when": { "north": "true" }, "apply": { "model": "jurassicrevived:block/low_security_fence_wire_part", "y":   0} },
    { "when": { "east":  "true" }, "apply": { "model": "jurassicrevived:block/low_security_fence_wire_part", "y":  90} },
    { "when": { "south": "true" }, "apply": { "model": "jurassicrevived:block/low_security_fence_wire_part", "y": 180} },
    { "when": { "west":  "true" }, "apply": { "model": "jurassicrevived:block/low_security_fence_wire_part", "y": 270} },
    { "when": { "ne": "true" }, "apply": { "model": "jurassicrevived:block/low_security_fence_wire_diagonal_part", "y":  90 } },
    { "when": { "se": "true" }, "apply": { "model": "jurassicrevived:block/low_security_fence_wire_diagonal_part", "y": 180 } },
    { "when": { "sw": "true" }, "apply": { "model": "jurassicrevived:block/low_security_fence_wire_diagonal_part", "y": 270 } },
    { "when": { "nw": "true" }, "apply": { "model": "jurassicrevived:block/low_security_fence_wire_diagonal_part", "y": 0 } }
  ]
}